@font-face {
  font-family: 'icon';
  src:  url('./assets/fonts/icon.eot?gihhr9');
  src:  url('./assets/fonts/icon.eot?gihhr9#iefix') format('embedded-opentype'),
    url('./assets/fonts/icon.woff2?gihhr9') format('woff2'),
    url('./assets/fonts/icon.ttf?gihhr9') format('truetype'),
    url('./assets/fonts/icon.woff?gihhr9') format('woff'),
    url('./assets/fonts/icon.svg?gihhr9#icon') format('svg');
